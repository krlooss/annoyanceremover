{
  "manifest_version": 3,
  "name": "Annoyance Remover",
  "version": "1.0",
  "description": "Removes annoying pop-ups, boxes, and toolbars from websites.",

  "permissions": [
    "tabs",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}